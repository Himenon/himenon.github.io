{
  "name": "himenon.github.io",
  "version": "1.0.0",
  "license": "UNLICENSED",
  "scripts": {
    "start": "yarn prebuild && yarn server",
    "prebuild": "run-p build:*",
    "server": "custom-site -c config.json -Do",
    "build": "yarn clean && custom-site -c config.json --out-dir docs",
    "build:lib": "yarn clean:lib && tsc -b tsconfig.json",
    "build:lib:watch": "yarn build:lib --watch",
    "clean:lib": "rimraf parts/**/*.js",
    "clean": "rimraf ./docs",
    "publish": "ts-node ./scripts/publish.ts",
    "lint": "eslint --cache --cache-location buildcache/ -c .eslintrc.json 'parts/**/*.{ts,tsx}'"
  },
  "dependencies": {
    "@custom-site/cli": "0.3.1",
    "latex.css": "1.1.0",
    "react": "16.8.6"
  },
  "devDependencies": {
    "@custom-site/interfaces": "0.3.1",
    "@types/gh-pages": "2.0.0",
    "@types/node": "11.13.4",
    "@types/prismjs": "1.16.0",
    "@types/react": "16.8.13",
    "@types/recursive-readdir": "2.2.0",
    "@types/url-join": "4.0.0",
    "@typescript-eslint/eslint-plugin": "1.9.0",
    "@typescript-eslint/eslint-plugin-tslint": "1.9.0",
    "@typescript-eslint/parser": "1.9.0",
    "dayjs": "1.8.14",
    "eslint": "5.16.0",
    "eslint-plugin-react": "7.13.0",
    "gh-pages": "2.0.1",
    "gray-matter": "4.0.2",
    "npm-run-all": "4.1.5",
    "prettier": "1.17.1",
    "pretty": "2.0.0",
    "prismjs": "1.16.0",
    "recursive-readdir": "2.2.2",
    "rimraf": "2.6.3",
    "simple-git": "1.113.0",
    "tslint": "5.16.0",
    "tslint-config-prettier": "1.18.0",
    "tslint-plugin-prettier": "2.0.1",
    "typescript": "3.4.3",
    "url-join": "4.0.0"
  }
}
