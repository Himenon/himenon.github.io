{
  "name": "himenon.github.io",
  "version": "1.0.0",
  "scripts": {
    "start": "yarn run prebuild && run-p server build:*:watch",
    "prebuild": "run-p build:*",
    "server": "custom-site -c config.json -Do",
    "build": "yarn run clean && custom-site -c config.json --out-dir docs",
    "build:lib": "yarn run clean:lib && tsc -b tsconfig.json",
    "build:lib:watch": "yarn run build:lib --watch",
    "clean:lib": "rimraf parts/**/*.js",
    "clean": "rimraf ./docs",
    "lint": "eslint --cache --cache-location buildcache/ -c .eslintrc.json 'parts/**/*.{ts,tsx}'"
  },
  "dependencies": {
    "@custom-site/cli": "^0.3.1",
    "react": "^16.8.6"
  },
  "devDependencies": {
    "@custom-site/interfaces": "^0.3.1",
    "@types/node": "^11.13.4",
    "@types/prismjs": "^1.16.0",
    "@types/react": "^16.8.13",
    "@types/url-join": "^4.0.0",
    "@typescript-eslint/eslint-plugin": "^1.9.0",
    "@typescript-eslint/eslint-plugin-tslint": "^1.9.0",
    "@typescript-eslint/parser": "^1.9.0",
    "eslint": "^5.16.0",
    "eslint-plugin-react": "^7.13.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.17.1",
    "pretty": "^2.0.0",
    "prismjs": "^1.16.0",
    "rimraf": "^2.6.3",
    "tslint": "^5.16.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.0.1",
    "typescript": "^3.4.3",
    "url-join": "^4.0.0"
  }
}
